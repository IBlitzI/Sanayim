{
  "expo": {
    "name": "Sanayim",
    "slug": "sanayim-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sanayim",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "projectId": "a064f946-4735-4723-90ce-348dce57bcfd",
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-image-picker",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a064f946-4735-4723-90ce-348dce57bcfd"
      }
    },
    "owner": "blitz19"
  }
}
